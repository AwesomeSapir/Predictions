<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<?import ui.component.custom.node.IconButton?>
<BorderPane fx:id="mainBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.component.main.MainController">
   <center>
       <TabPane fx:id="mainTabPane" tabClosingPolicy="UNAVAILABLE">
           <opaqueInsets>
               <Insets />
           </opaqueInsets>
           <Tab fx:id="tabDetailsID" text="Details">
               <fx:include fx:id="tabDetails" source="/ui/component/subcomponent/detail/tabDetails.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
           </Tab>
           <Tab fx:id="tabExecutionID" text="New Execution">
               <fx:include fx:id="tabExecution" source="/ui/component/subcomponent/execution/tabExecution.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
           </Tab>
           <Tab fx:id="tabResultsID" text="Results">
               <fx:include fx:id="tabResults" source="/ui/component/subcomponent/result/tabResults.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
           </Tab>
       </TabPane>
   </center>
   <top>
       <GridPane hgap="8.0" BorderPane.alignment="CENTER">
           <columnConstraints>
               <ColumnConstraints hgrow="NEVER" />
               <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
               <ColumnConstraints hgrow="NEVER" />
               <ColumnConstraints hgrow="NEVER" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
           <padding>
               <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
           </padding>
           <IconButton fx:id="chooseFileButton" icon="icon-file-open" mnemonicParsing="false" onAction="#chooseXMLFile" text="LOAD FILE" GridPane.columnIndex="0" />
           <TextField fx:id="filePathTextField" editable="false" prefHeight="26.0" prefWidth="153.0" promptText="Current Loaded File Path" GridPane.columnIndex="1" />
           <IconButton fx:id="queueManagementButton" icon="icon-queue" mnemonicParsing="false" text="QUEUE" GridPane.columnIndex="2" />
           <IconButton fx:id="buttonSettings" icon="icon-settings" mnemonicParsing="false" styleClass="button-circle" GridPane.columnIndex="3"/>
       </GridPane>
   </top>
</BorderPane>
