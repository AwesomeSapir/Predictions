<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="8.0" vgap="8.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.component.custom.detail.property.PropertyDetailView">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="50.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" percentWidth="50.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
        <RowConstraints vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
    </padding>
    <Label text="Name" GridPane.vgrow="ALWAYS">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <Label fx:id="labelName" text="Name Value" GridPane.columnIndex="1"/>
    <Label text="Type" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <Label fx:id="labelType" text="Type Value" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label text="Range" GridPane.rowIndex="2">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <Label fx:id="labelRange" text="Range Value" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Label text="Random?" GridPane.rowIndex="3">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </Label>
    <Label fx:id="labelRandom" text="Random Value" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
</GridPane>
