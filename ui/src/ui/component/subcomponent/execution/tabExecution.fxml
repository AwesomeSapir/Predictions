<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.component.subcomponent.execution.ExecutionController">

    <bottom>
        <AnchorPane minHeight="-Infinity" prefHeight="64.0" prefWidth="599.0" BorderPane.alignment="CENTER">
            <Button fx:id="buttonClear" layoutX="14.0" layoutY="19.0" mnemonicParsing="false" text="CLEAR"
